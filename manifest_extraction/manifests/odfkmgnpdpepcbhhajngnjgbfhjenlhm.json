{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "jquery.min.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "Boxmyjob"
  },
  "name": "__MSG_appName__",
  "short_name": "Boxmyjob",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "homepage_url": "https://boxmyjob.com?ref=chromestore&from=chromestore",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "version": "1.2.4",
  "manifest_version": 2,
  "web_accessible_resources": [
    "manifest.json",
    "bookmarklet.js"
  ]
}