{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Screenr",
  "version": "0.2.19",
  "description": "The easiest screen capture and annotation tool in the universe!",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "screenr_logo16.png",
    "48": "screenr_logo48.png",
    "128": "screenr_logo128.png"
  },
  "browser_action": {
    "default_icon": "screenr_logo32.png",
    "default_title": "Capture page!"
  },
  "permissions": [
    "activeTab",
    "identity"
  ],
  "manifest_version": 2,
  "oauth2": {
    "client_id": "337350846948-7a8bkjeptfp4s0p1ch84r303jp6eb20n.apps.googleusercontent.com",
    "scopes": [
      "openid",
      "email",
      "profile"
    ]
  },
  "key": "",
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://shots.screenr.co/*"
    ]
  }
}