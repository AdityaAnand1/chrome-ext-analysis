{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Start - A Better New Tab",
  "version": "3.14.7",
  "manifest_version": 2,
  "description": "Hands down, the best home page for Chrome. Beautiful wallpapers, amazing features, blazingly fast!",
  "background": {
    "page": "background.html"
  },
  "key": "",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Open New Tab"
  },
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "options_page": "tab.html#options",
  "permissions": [
    "geolocation",
    "topSites",
    "management",
    "unlimitedStorage",
    "storage",
    "<all_urls>",
    "notifications",
    "idle",
    "identity",
    "webRequest",
    "webRequestBlocking",
    "alarms"
  ],
  "oauth2": {
    "client_id": "777517346627-8p0s5kg8pc4gandcfhrhogejp0cq19d6.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar",
      "https://www.googleapis.com/auth/gmail.readonly"
    ]
  },
  "optional_permissions": [
    "bookmarks",
    "tabs",
    "sessions"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://static.webtrackinganalytics.com; object-src 'self'"
}