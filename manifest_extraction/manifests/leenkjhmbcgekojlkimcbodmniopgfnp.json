{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "New Tab: Winter Animation",
  "short_name": "Winter N.T.",
  "description": "Winter Animated New Tab, a Chrome-based personalised dashboard.",
  "version": "4.2",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "background",
    "http://*/*",
    "https://*/*",
    "chrome://favicon/",
    "topSites",
    "tabs",
    "geolocation",
    "unlimitedStorage",
    "<all_urls>",
    "storage",
    "bookmarks",
    "management",
    "https://ajax.googleapis.com/",
    "https://www.google-analytics.com/"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://www.google.com https://query.yahooapis.com; object-src 'self'",
  "offline_enabled": true
}