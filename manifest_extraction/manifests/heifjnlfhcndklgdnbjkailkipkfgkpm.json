{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Weatherly",
  "version": "1.6",
  "description": "This extension provide information about (location, wind, atmosphere, astronomy, condition and forecasts!)",
  "options_page": "settings.html",
  "background": {
    "scripts": [
      "scripts/jquery.min.js",
      "scripts/badge.js"
    ],
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "Icon16.png",
      "32": "Icon32.png",
      "48": "Icon48.png",
      "128": "Icon128.png"
    }
  },
  "icons": {
    "16": "Icon16.png",
    "32": "Icon32.png",
    "48": "Icon48.png",
    "128": "Icon128.png"
  },
  "permissions": [
    "https://*/",
    "http://*/",
    "https://www.google-analytics.com/*",
    "activeTab",
    "storage"
  ],
  "manifest_version": 2,
  "author": "Rafi Haidari"
}