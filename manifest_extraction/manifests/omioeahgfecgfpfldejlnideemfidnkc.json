{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Google Publisher Toolbar",
  "short_name": "Pub Toolbar",
  "version": "5.2.0",
  "description": "__MSG_app_description__",
  "manifest_version": 2,
  "default_locale": "en",
  "minimum_chrome_version": "20",
  "incognito": "split",
  "icons": {
    "16": "icon-16x16.png",
    "48": "icon-48x48.png",
    "128": "icon-128x128.png"
  },
  "offline_enabled": false,
  "background": {
    "scripts": [
      "external_background_bin.js"
    ]
  },
  "options_page": "external_options.html",
  "browser_action": {
    "default_icon": "icon-19x19-offline.png",
    "default_title": "Google Publisher Toolbar",
    "default_popup": "external_popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.google.com/adsense/*",
        "https://*.google.com/adxseller/*"
      ],
      "js": [
        "contentscript_install_check_bin.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "external_contentscript_siteoverlay_bin.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "https://accounts.google.com/o/oauth2/approval?*",
        "https://accounts.google.com/o/oauth2/approval/v2/approvalnativeapp?*"
      ],
      "js": [
        "contentscript_oauth_response_handler.js"
      ],
      "all_frames": true,
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "tabs",
    "windows",
    "background",
    "storage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "notifications",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://apis.google.com; img-src 'self' data: http://www.google-analytics.com https://*.google.com https://csi.gstatic.com https://ssl.gstatic.com; style-src 'self' https://fonts.googleapis.com/css data: 'unsafe-inline'; frame-src https://*.google.com https://content.googleapis.com; font-src https://fonts.gstatic.com; object-src 'self'; connect-src https://*.google.com; default-src 'self'",
  "web_accessible_resources": [
    "block.svg",
