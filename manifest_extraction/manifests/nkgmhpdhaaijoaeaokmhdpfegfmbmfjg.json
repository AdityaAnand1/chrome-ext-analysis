{
  "background": {
    "persistent": false,
    "scripts": [
      "/assets/js/jquery.min.js",
      "pixlr.js",
      "background.min.js"
    ]
  },
  "browser_action": {
    "default_title": "Pixlr Today"
  },
  "chrome_url_overrides": {
    "newtab": "dashboard.html"
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://query.yahooapis.com https://*.123rf.com 'unsafe-eval'; object-src 'self'",
  "description": "Experience new Chrome Tabs with Pixlr Today.",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "name": "Pixlr Today",
  "offline_enabled": true,
  "permissions": [
    "geolocation",
    "unlimitedStorage",
    "*://*.pixlr.com/*",
    "*://www.123rfapis.com/*",
    "history",
    "management",
    "topSites",
    "chrome://favicon/"
  ],
  "short_name": "Pixlr Today",
  "author": "Pixlr",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.14"
}