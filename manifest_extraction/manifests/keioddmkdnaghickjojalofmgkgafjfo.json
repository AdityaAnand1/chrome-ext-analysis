{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "DataMask",
  "short_name": "DataMask",
  "description": "Mask private data before taking a screenshot",
  "version": "1.4.2",
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  },
  "icons": {
    "19": "icons/icon19.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png"
  },
  "commands": {
    "Open DataMask": {
      "suggested_key": {
        "default": "Ctrl+M",
        "mac": "Command+M"
      },
      "description": "Opens DataMask banner"
    }
  },
  "permissions": [
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "web_accessible_resources": [
    "icons/icon128.png"
  ]
}