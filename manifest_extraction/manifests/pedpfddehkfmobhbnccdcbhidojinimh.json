{
  "background": {
    "scripts": [
      "adguard/adguard-api.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "img/icon_256.png",
    "default_popup": "popup.html",
    "default_title": "Video Adblocker"
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "default_locale": "en_US",
  "description": "Video AdBlocker",
  "icons": {
    "256": "img/icon_256.png"
  },
  "manifest_version": 2,
  "name": "Video Adblocker",
  "permissions": [
    "webRequest",
    "<all_urls>",
    "cookies",
    "storage",
    "tabs",
    "activeTab",
    "webRequestBlocking",
    "webNavigation",
    "management",
    "notifications"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.1"
}