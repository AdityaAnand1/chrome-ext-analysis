{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "mixMovie Start",
  "description": "mixMovie Start is a new Chrome extension for a quick movies,trailers and actress click-and-visit experience.",
  "version": "1.0.2",
  "short_name": "mixMovie",
  "icons": {
    "128": "imgs/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "/js/*"
  ],
  "permissions": [
    "http://*/",
    "tabs",
    "storage",
    "cookies"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "background": {
    "scripts": [
      "/js/background.js"
    ]
  }
}