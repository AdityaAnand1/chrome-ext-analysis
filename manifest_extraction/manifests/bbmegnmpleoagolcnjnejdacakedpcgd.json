{
  "background": {
    "page": "Plugin/background.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "Plugin/Ex.js",
        "Plugin/FingerPrint.js",
        "adblock/js/port.js",
        "adblock/js/functions.js",
        "adblock/filtering/filteroptions.js",
        "adblock/js/adblock_start_common.js",
        "adblock/js/adblock_start_chrome.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "Protect your browsing against online threats, phishing attacks, cryptocurrency mining web pages, and annoying ads.",
  "icons": {
    "128": "Plugin/img/asc.png",
    "48": "Plugin/img/asc.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "IObit Surfing Protection & Ads Removal",
  "browser_action": {
    "default_icon": "Plugin/img/icon_ok_gry_16.png",
    "default_popup": "plugin/safe.html",
    "default_title": "IObit Surfing Protection & Ads Removal"
  },
  "permissions": [
    "nativeMessaging",
    "tabs",
    "*://*/*",
    "storage",
    "unlimitedStorage",
    "contextMenus",
    "tabs",
    "idle",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "management"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.2.2",
  "web_accessible_resources": [
    "Plugin/*",
    "Plugin/img/*",
    "adblock/*"
  ]
}