{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "ScreenShot & Screen Capture Elite",
  "short_name": "Screenshot",
  "version": "1.1.0",
  "description": "ScreenShot & Screen Capture Elite Is The Best Screen Shot Extension - Full Page Screen Capture, Webpage Screenshot And More!",
  "minimum_chrome_version": "22",
  "background": {
    "persistent": true,
    "scripts": [
      "js/jquery.js",
      "js/background.js",
      "js/cropper.js",
      "js/jquery-cropper.js"
    ]
  },
  "browser_action": {},
  "icons": {
    "24": "24_icon.png",
    "128": "128_icon.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "tabCapture",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "webNavigation",
    "unlimitedStorage",
    "debugger"
  ],
  "externally_connectable": {
    "matches": [
      "*://67.205.139.234/*"
    ]
  }
}